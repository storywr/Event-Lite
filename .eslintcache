event: Event) => {\n    setEvent(event)\n    onDeleteOpen()\n  }\n\n  const handleEditClick = (event: Event) => {\n    setEvent(event)\n    onEditOpen()\n  }\n\n  if (isFetching) return <Spinner />\n  if (error) return <Alert />\n\n  return (\n    <Box display='block'>\n      <Modals\n        event={selectedEvent}\n        isOpen={{\n          add: isAddOpen,\n          edit: isEditOpen,\n          delete: isDeleteOpen\n        }}\n        onClose={{\n          add: onAddClose,\n          edit: onEditClose,\n          delete: onDeleteClose\n        }}\n      />\n      <Box mb='2rem' w='500px'>\n        <FormLabel><SearchIcon /> Search for Event</FormLabel>\n        <InputGroup>\n          <Input\n            name='title'\n            placeholder='Search'\n            value={search}\n            onChange={e => setSearch(e.target.value)}\n          />\n          {search &&\n            <InputRightElement>\n              <IconButton\n                variant='ghost'\n                aria-label=\"Clear search\"\n                icon={<CloseIcon />}\n                onClick={() => setSearch('')}\n              />\n            </InputRightElement>\n          }\n        </InputGroup>\n      </Box>\n      <SimpleGrid mb='3rem' columns={3} spacing={35}>\n        {data.map((event: Event) => (\n          <EventCard\n            event={event}\n            handleDeleteClick={handleDeleteClick}\n            handleEditClick={handleEditClick}\n          />\n        ))}\n        <Button\n          onClick={() => onAddOpen()}\n          as={Button}\n          p='2rem'\n          minW='500px'\n          minH='500px'\n          borderWidth='1px'\n        >\n          <AddIcon m='auto'/>\n        </Button>\n      </SimpleGrid>\n    </Box>\n  )\n}\n\nexport default Dashboard",["138","139"],"/Users/bstory/eventlite-example/src/util/localstore.ts",[],"/Users/bstory/eventlite-example/src/components/Navbar/index.tsx",[],"/Users/bstory/eventlite-example/src/components/ThemeSelector/index.tsx",