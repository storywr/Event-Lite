start_datetime: string\n}\n\nconst useEvents = (search: string) => {\n  return useQuery('events', async () => {\n    const { data } = await axios({\n      method: 'GET',\n      url: `http://localhost:3001/events?search=${search}`,\n      headers: JSON.parse(localStorage.user)\n    })\n    return data\n  })\n}\n\nconst Dashboard = () => {\n  const [search, setSearch] = useState('')\n  const [selectedEvent, setEvent] = useState<any>(null)\n  const debouncedValue = useDebouncedValue(search, 500)\n  const { data, error, isFetching, refetch } = useEvents(debouncedValue)\n\n  useEffect(() => {\n    refetch()\n  }, [debouncedValue])\n\n  const { isOpen: isDeleteOpen, onOpen: onDeleteOpen, onClose: onDeleteClose } = useDisclosure()\n  const { isOpen: isEditOpen, onOpen: onEditOpen, onClose: onEditClose } = useDisclosure()\n  const { isOpen: isAddOpen, onOpen: onAddOpen, onClose: onAddClose } = useDisclosure()\n\n  const handleDeleteClick = (event: Event) => {\n    setEvent(event)\n    onDeleteOpen()\n  }\n\n  const handleEditClick = (event: Event) => {\n    setEvent(event)\n    onEditOpen()\n  }\n\n  if (isFetching) return <Spinner />\n  if (error) return <Alert />\n\n  return (\n    <Box display='block'>\n      <DeleteModal\n        event={selectedEvent}\n        isOpen={isDeleteOpen}\n        onClose={onDeleteClose}\n      />\n      <EditModal\n        event={selectedEvent}\n        isOpen={isEditOpen}\n        onClose={onEditClose}\n      />\n      <AddModal\n        isOpen={isAddOpen}\n        onClose={onAddClose}\n      />\n      <Box mb='2rem' w='480px'>\n        <FormLabel><SearchIcon /> Search for Event</FormLabel>\n        <InputGroup>\n          <Input\n            name='title'\n            placeholder='Search'\n            value={search}\n            onChange={e => setSearch(e.target.value)}\n          />\n          {search &&\n            <InputRightElement>\n              <IconButton\n                variant='ghost'\n                aria-label=\"Clear search\"\n                icon={<CloseIcon />}\n                onClick={() => setSearch('')}\n              />\n            </InputRightElement>\n          }\n        </InputGroup>\n      </Box>\n      <SimpleGrid mb='3rem' columns={3} spacing={35}>\n        {data.map((event: Event) => (\n          <Box\n            boxShadow='md'\n            rounded='lg'\n            p='1rem'\n            minW='480px'\n            minH='480px'\n            maxH='480px'\n            maxW='480px'\n            borderWidth='1px'\n            overflow='scroll'\n            key={event.id}\n          >\n            <Flex justifyContent='space-between'>\n              <Flex\n                textTransform='uppercase'\n                fontSize='md'\n                fontWeight='bold'\n                maxW='80%'\n                flexWrap='wrap'\n              >\n                {event.title}\n              </Flex>\n              <Menu>\n                <MenuButton variant='ghost' as={Button}>\n                  <HamburgerIcon />\n                </MenuButton>\n                <MenuList>\n                  <MenuItem onClick={() => handleEditClick(event)}>Edit</MenuItem>\n                  <MenuItem onClick={() => handleDeleteClick(event)}>Delete</MenuItem>\n                </MenuList>\n              </Menu>\n            </Flex>\n            <Flex\n              alignItems='center'\n              fontWeight='semibold'\n              mt='0.5rem'\n              maxW='80%'\n              flexWrap='wrap'\n            >\n              <StarIcon mr='0.5rem'/> {event.location}\n            </Flex>\n            <Flex\n              alignItems='center'\n              fontWeight='semibold'\n              mt='0.5rem'\n              maxW='80%' \n              flexWrap='wrap'\n            >\n              <CalendarIcon mr='0.5rem' />{format(new Date(event['start_datetime']), 'M/d/yyyy, h:mm aa')}\n            </Flex>\n            <StyledBox\n              mt='1rem'\n              flexWrap='wrap'\n            >\n              {event.description}\n            </StyledBox>\n          </Box>\n        ))}\n        <Button\n          onClick={() => onAddOpen()}\n          as={Button}\n          p='2rem'\n          minW='480px'\n          minH='480px'\n          borderWidth='1px'\n        >\n          <AddIcon m='auto'/>\n        </Button>\n      </SimpleGrid>\n    </Box>\n  )\n}\n\nexport default Dashboard","/Users/bstory/eventlite-example/src/util/localstore.ts",["110"],"export const ACCESS_TOKEN = 'token'\nexport const REFRESH_TOKEN = 'refreshToken'\n\nexport default {\n  clear: () => window.localStorage.clear(),\n  get: (key: string) => window.localStorage.getItem(key),\n  remove: (key: string) => window.localStorage.removeItem(key),\n  set: (key: string, value: any) => window.localStorage.setItem(key, value)\n}",["111","112"],"/Users/bstory/eventlite-example/src/components/Navbar/index.tsx",[],"/Users/bstory/eventlite-example/src/components/ThemeSelector/index.tsx",[],"/Users/bstory/eventlite-example/src/components/Alert/index.tsx",[],"/Users/bstory/eventlite-example/src/pages/Dashboard/EditModal/index.tsx",[],"/Users/bstory/eventlite-example/src/pages/Dashboard/DeleteModal/index.tsx",[],"/Users/bstory/eventlite-example/src/pages/Dashboard/EditModal/EditForm.tsx",[],"/Users/bstory/eventlite-example/src/pages/Dashboard/AddModal/AddForm.tsx",[],"/Users/bstory/eventlite-example/src/pages/Dashboard/AddModal/index.tsx",[],"/Users/bstory/eventlite-example/src/serviceWorker.ts",[],["113","114"],"/Users/bstory/eventlite-example/src/hooks/useDebouncedValue.ts",["115"],"/Users/bstory/eventlite-example/src/pages/Authentication/index.tsx",[],"/Users/bstory/eventlite-example/src/pages/Signup/index.tsx",[],"/Users/bstory/eventlite-example/src/pages/Signup/SignupForm.tsx",[],"/Users/bstory/eventlite-example/src/pages/Signup/SignupHeader.tsx",[],{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","severity":1,"message":"121","line":69,"column":6,"nodeType":"